    light.0.i="0.8"
    light.0.a="30"
    light.0.b="10"
    light.1.i="0.5"
    tierHeight="4.5"
    incarnHeight="tierHeight * 4"
  />

  <Sound y="100" sound="snArcticWind" isAmbient="true" loopCount="-1" volume="100" cx="0" cz="0" angle="45" />

  <Teleporter activeRange="175" deadRange="125" cx="0" cz="0" destGroup="@recenter" group="-1" shape="0" spin="true" y="-30" />
